<h2>My listing</h2>

<%= link_to "Add new item", new_item_path %>

<div id="user_items">
  <% @items.each do |item| %>
    <div class="item">
      <p class="name"><%= item.name %></p>
      <p class="description"><%= item.description %></p>
      <p class="category"><%= item.category %></p>
      <p><%= link_to "Edit item", edit_item_path(item) %></p>
    </div>
  <% end %>
</div>

